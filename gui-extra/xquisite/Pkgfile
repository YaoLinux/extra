makedepends=(gtk-update-icon-cache)

description="Icons xquisite for xfce"
url="http://xfce-look.org/content/show.php/Xquisite?content=69735"

packager="Yaolinux Team"
maintainer="rems"

name=xquisite
version=0.4.6
release=2

run=(gtk-update-icon-cache)

source=(http://download2146.mediafire.com/w18544rcl85g/lozb6ec3no9a233/Xquisite.tbz)

build() {
mkdir -p $PKG//usr/share/icons/
mv Xquisite $PKG//usr/share/icons/
}
